#ifndef ARTICULOS_H_
#define ARTICULOS_H_
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <ctype.h>

typedef struct
{
	int id;
	char articulo[50];
	char medida[50];
	float precio;
	int rubroId;


}eArticulos;
eArticulos* articulos_new();
eArticulos* articulos_newParametros(char* id, char* articulo, char* medida, char* precio, char* rubroId);





#endif /* ARTICULOS_H_ */
